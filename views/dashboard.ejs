

<!DOCTYPE html>
<html>
    <head>
        <title>Dashboard</title>
        <meta charset="utf-8">
        <title>Express Application</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    </head>
    <body>
        <h2>Welcome to Dashboard</h2>
        <p><b>Login Email</b> - <%=email %></p>
        <p><a href="/logout">Logout</a></p>

        <form class="search" action="search" method="post">
            <input class="form-control typeahead tt-query" type="text" name="input" placeholder="Search">
            <input class="btn btn-primary" type="submit"  value="Submit">
            <p><%=(typeof alertMsg!='undefined')? alertMsg:''%>
        </form>
        <div>
            
            <table>
                <tr>
                    <th width="50px">No</th>
                    <th>Email</th>
                    <th></th>
                    <th></th>
                    <th></th>
                    <th></th>
                    <th width="120px">Action</th>
                </tr>
    
                <% if(data.length){
    
                for(var i = 0;i < data.length;i++) { %>
    
                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].Email%></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td>
                        <a class="a-inside edit" href="../employees/edit/<%=data[i].id%>">Edit</a>
                        <a class="a-inside delete" href="../employees/delete/<%=data[i].id%>">Delete</a>
                    </td>
                </tr>
                <% }
                }else{ %>
                <tr>
                    <td colspan="6">No user</td>
                </tr>
                <% } %>
            </table>
        </div>
    </body>
</html>
